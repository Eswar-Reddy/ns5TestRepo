    <validationRules>
        <fullName>PG_set_madatory_on_PG_stage</fullName>
        <active>true</active>
        <description>The &quot;Playground Set&quot; field is mandatory when stage is Playground</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Playground&apos;),ISBLANK(TEXT( Playground_Set__c)))</errorConditionFormula>
        <errorDisplayField>Playground_Set__c</errorDisplayField>
        <errorMessage>Playground set is a mandatory field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premium_support_field_mandatory</fullName>
        <active>true</active>
        <description>This rule will make the premium support field mandatory if the stage is set to Closed Won</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Closed Won&apos;),ISBLANK(TEXT( Is_eligibile_for_Premier_support__c)))</errorConditionFormula>
        <errorDisplayField>Is_eligibile_for_Premier_support__c</errorDisplayField>
        <errorMessage>This field is required for Closed Won Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VC_Type_mandatory_on_PG_stage</fullName>
        <active>true</active>
        <description>The &quot;Version Control Type&quot; field is mandatory on Playground stage.</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Playground&apos;),ISBLANK(TEXT( Version_Control_Type__c )))</errorConditionFormula>
        <errorDisplayField>Version_Control_Type__c</errorDisplayField>
        <errorMessage>Version Control Type is a mandatory field</errorMessage>
    </validationRules>
